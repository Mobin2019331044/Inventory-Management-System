                                <path
                                  d="M17.414 2.586a2 2 0 00-2.828 0L7 10.172V13h2.828l7.586-7.586a2 2 0 000-2.828z"
                                ></path>
                                <path
                                  fill-rule="evenodd"
                                  d="M2 6a2 2 0 012-2h4a1 1 0 010 2H4v10h10v-4a1 1 0 112 0v4a2 2 0 01-2 2H4a2 2 0 01-2-2V6z"
                                  clip-rule="evenodd"
                                ></path>
                              </svg>
                              View Invoice
                            </button>
                          </a>
                          <a
                            href="/duePayment?data=<%= encodeURIComponent(JSON.stringify(row)) %>"
                          >
                            <button
                              type="button"
                              id="openModalButton"
                              data-drawer-target="drawer-update-product-default"
                              data-drawer-show="drawer-update-product-default"
                              aria-controls="drawer-update-product-default"
                              data-drawer-placement="right"
                              class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white rounded-lg bg-yellow-700 hover:bg-yellow-800 focus:ring-4 focus:ring-primary-300 dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800"
                            >
                              <svg
                                class="w-4 h-4 mr-2"
                                fill="currentColor"
                                viewBox="0 0 20 20"
                                xmlns="http://www.w3.org/2000/svg"
                              >
                                <path
                                  d="M17.414 2.586a2 2 0 00-2.828 0L7 10.172V13h2.828l7.586-7.586a2 2 0 000-2.828z"
                                ></path>
                                <path
                                  fill-rule="evenodd"
                                  d="M2 6a2 2 0 012-2h4a1 1 0 010 2H4v10h10v-4a1 1 0 112 0v4a2 2 0 01-2 2H4a2 2 0 01-2-2V6z"
                                  clip-rule="evenodd"
                                ></path>
                              </svg>
                              Due Payment
                            </button>
                          </a>
                        </td>
                      </tr>

                      <!-- Modal -->
                      <div
                        id="modal"
                        class="bg-white p-6 rounded-md shadow-md z-20 hidden"
                      >
                        <h2 class="text-xl font-semibold mb-4">Modal Title</h2>
                        <p>This is the content of the modal.</p>
                        <button
                          id="closeModal"
                          class="mt-4 bg-gray-300 hover:bg-gray-400 px-4 py-2 rounded-md"
                        >
                          Close
                        </button>
                      </div>

                      <!-- Open Modal Button -->

                      <script>
                        const openModalButton =
                          document.getElementById("openModalButton");
                        const modalBackground =
                          document.getElementById("modalBackground");
                        const modal = document.getElementById("modal");
                        const closeModalButton =
                          document.getElementById("closeModal");

                        openModalButton.addEventListener("click", () => {
                          modalBackground.classList.remove("hidden");
                          modal.classList.remove("hidden");
                        });

                        closeModalButton.addEventListener("click", () => {
                          modalBackground.classList.add("hidden");
                          modal.classList.add("hidden");
                        });
                      </script>

                      <% }) %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </main>
      </div>
    </div>

