        <script>
            const openModal1Button = document.getElementById("openModal1");
            const closeModal1Button = document.getElementById("closeModal1");
            const modal1 = document.getElementById("modal1");

            openModal1Button.addEventListener("click", () => {
                modal1.classList.remove("hidden");
            });

            closeModal1Button.addEventListener("click", () => {
                modal1.classList.add("hidden");
            });

            const openModal2Button = document.getElementById("openModal2");
            const closeModal2Button = document.getElementById("closeModal2");
            const modal2 = document.getElementById("modal2");

            openModal2Button.addEventListener("click", () => {
                modal2.classList.remove("hidden");
            });

            closeModal2Button.addEventListener("click", () => {
                modal2.classList.add("hidden");
            });

            // Get references to elements
            const searchInput = document.querySelector("#search");
            const resultsBody = document.querySelector("#results");

            // Initial load of data
            load_data();

            // Load data function
            function load_data(query = "") {
                const request = new XMLHttpRequest();

                // Configure the request
                request.open("GET", `/search?q=${query}`);

                // When the request is complete
                request.onload = () => {
                    const results = JSON.parse(request.responseText);
                    let html = "";

                    // Generate HTML for each result
                    if (results.length > 0) {
                        results.forEach((result) => {
                            html += ` <tr class="hover:bg-gray-200 dark:hover:bg-gray-700">
                        <td class="w-4 p-4">
                          <div class="flex items-center">
                            <svg 
                              fill="#000000"
                              width="30px"
                              height="30px"
                              viewBox="-2 -2 24.00 24.00"
                              xmlns="http://www.w3.org/2000/svg"
                              stroke="#000000"
                              stroke-width="2"
                            >
                              <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                              <g
                                id="SVGRepo_tracerCarrier"
                                stroke-linecap="round"
                                stroke-linejoin="round"
                                stroke="#CCCCCC"
                                stroke-width="0.04"
                              ></g>
                              <g id="SVGRepo_iconCarrier">
                                <path
                                  d="M7.8 10a2.2 2.2 0 0 0 4.4 0 2.2 2.2 0 0 0-4.4 0z"
                                ></path>
                              </g>
                            </svg>
                          </div>
                        </td>
                        <td
                          class="flex p-4 text-base font-medium text-gray-900 whitespace-nowrap dark:text-white"
                        >
                          <img
                            class="w-10 h-10 rounded-full"
                            src="/images/product_demo.png"
                            alt="Neil Sims avatar"
                          />

                          <div
                            class="ml-2 text-sm font-normal text-gray-500 dark:text-gray-400"
                          >
                            <div
                              class="text-base font-semibold text-gray-900 dark:text-white"
                            >
                            ${result.p_name}
                            </div>
                            <div
                              class="overflow-hidden text-base font-normal text-gray-500 truncate xl:max-w-xs dark:text-gray-400"
                            >
                            ${result.p_des}
                            </div>
                          </div>
                        </td>
                        <td
                          class="p-4 text-base font-medium text-gray-900 whitespace-nowrap dark:text-white"
                        >
                        ${result.p_code}
                        </td>
                        <td
                          class="p-4 text-base font-medium text-gray-900 whitespace-nowrap dark:text-white"
                        >
                        ${result.p_category}
                        </td>

                        <td
                          class="p-4 text-base font-medium text-gray-900 whitespace-nowrap dark:text-white"
                        >
                        ${result.total_amount}
                        </td>
                        <td
                          class="p-4 text-base font-medium text-gray-900 whitespace-nowrap dark:text-white"
                        >
                        $ ${result.p_price}
                        </td>
                        <td class="p-4 space-x-2 whitespace-nowrap">
                          
                          <a 
                          href="/changeProductsState?data=${encodeURIComponent(
                              JSON.stringify(result)
                          )}" >
                          
                          <button
                            type="button"
                            id="updateProductButton"
                            data-drawer-target="drawer-update-product-default"
                            data-drawer-show="drawer-update-product-default"
                            aria-controls="drawer-update-product-default"
                            data-drawer-placement="right"
                            class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white rounded-lg bg-yellow-700 hover:bg-yellow-800 focus:ring-4 focus:ring-primary-300 dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800"
                          >
                            <svg
                              class="w-4 h-4 mr-2"
                              fill="currentColor"
                              viewBox="0 0 20 20"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <path
                                d="M17.414 2.586a2 2 0 00-2.828 0L7 10.172V13h2.828l7.586-7.586a2 2 0 000-2.828z"
                              ></path>
                              <path
                                fill-rule="evenodd"
                                d="M2 6a2 2 0 012-2h4a1 1 0 010 2H4v10h10v-4a1 1 0 112 0v4a2 2 0 01-2 2H4a2 2 0 01-2-2V6z"
                                clip-rule="evenodd"
                              ></path>
                            </svg>
                            cart
                          </button>
                          
                        </a>
                          <button
                            type="button"
                            id="deleteProductButton"
                            data-drawer-target="drawer-delete-product-default"
                            data-drawer-show="drawer-delete-product-default"
                            aria-controls="drawer-delete-product-default"
                            data-drawer-placement="right"
                            class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-red-700 rounded-lg hover:bg-red-800 focus:ring-4 focus:ring-red-300 dark:focus:ring-red-900"
                          >
                            <svg
                              class="w-4 h-4 mr-2"
                              fill="currentColor"
                              viewBox="0 0 20 20"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <path
                                fill-rule="evenodd"
                                d="M9 2a1 1 0 00-.894.553L7.382 4H4a1 1 0 000 2v10a2 2 0 002 2h8a2 2 0 002-2V6a1 1 0 100-2h-3.382l-.724-1.447A1 1 0 0011 2H9zM7 8a1 1 0 012 0v6a1 1 0 11-2 0V8zm5-1a1 1 0 00-1 1v6a1 1 0 102 0V8a1 1 0 00-1-1z"
                                clip-rule="evenodd"
                              ></path>
                            </svg>
                            Damaged
                          </button>
                        </td>
                      </tr>
                      `;
                        });
                    } else {
                        // No results found
                        html += `
                <tr>
                    <td colspan="5" class="text-center">No Data Found</td>
                </tr>`;
                    }

                    // Update the results table
                    resultsBody.innerHTML = html;
                };

                // Send the request
                request.send();
            }

            // Listen for input changes in the search field
            searchInput.addEventListener("input", () => {
                const query = searchInput.value;
                load_data(query);
            });
        </script>

        <!-- <script async defer src="https://buttons.github.io/buttons.js "></script> -->
